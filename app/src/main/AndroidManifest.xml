<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.testexercise.hauteloockcdn">

    <uses-permission android:name="android.permission.INTERNET" />
    <application
        android:name="com.testexercise.hauteloockcdn.app.AppController"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name="com.testexercise.hauteloockcdn.Main.MainActivity"
            android:label="@string/app_name"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.testexercise.hauteloockcdn.Main.Article"
            android:label="Article" >
        </activity>
    </application>
</manifest>
